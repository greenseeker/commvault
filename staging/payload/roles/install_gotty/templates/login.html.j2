        <title>{{ host }}</title>
        <style>
            @import url('https://fonts.googleapis.com/css2?family=Fira+Code&display=swap');
            @import url('https://fonts.googleapis.com/css2?family=Rubik&display=swap');

            body {
                padding: 0;
                margin: 0;
            }

            /* The sidebar throws off the vertical alignment of the size/cut notification;
               the following property fixes it. */
             div#terminal > div {
                top: 45% !important;
            }

            .tooltip {
                position: relative;
                display: inline-block;
            }

            .tooltip .tooltiptext {
                font-family: Rubik,Tahoma,sans-serif;
                visibility: hidden;
                white-space: nowrap;
                background-color: rgba(223,223,223,1);
                color: #000;
                text-align: center;
                padding: 5px;
                border-radius: 6px;
                position: absolute;
                right: 105%;
                top: 15px;
                font-size: 11pt;
            }

            .tooltip:hover .tooltiptext {
                transition-delay: .75s;
                visibility: visible;
            }

            .tooltip .tooltiptext::after {
                content: " ";
                position: absolute;
                top: 50%;
                left: 100%;
                margin-top: -5px;
                border-width: 5px;
                border-style: solid;
                border-color: transparent transparent transparent rgba(223,223,223,1);
            }

            .sidebar {
                position: fixed;
                z-index: 1;
                top: 0;
                right: 25px;
                text-align: right;
            }

            .sidebar a {
                padding: 8px;
                text-decoration: none;
                margin-left: auto;
                margin-right: 0;
                display: block;
            }

            .sidebar a svg {
                fill: rgba(223,223,223,.1);
                width: 50px;
                height: 40px;
                transition: .25s;
            }

            .sidebar:hover a svg {
                fill: rgba(223,223,223,.75);
                transition: .25s;
            }

            .sidebar a:hover svg {
                fill: rgba(255,74,106,1);
                transition: .25s;
            }


        </style>
            <link rel="icon" type="image/png" href="favicon.png">
